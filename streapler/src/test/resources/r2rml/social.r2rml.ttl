@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix sr: <http://purl.oclc.org/rsp/srml#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix ex: <http://example.org/rsp/>.
@prefix : <http://epfl.ch/mapping/>.

:PersonRoomMap  
  rml:logicalSource [rr:tableName "social"; rml:source ""; rml:root true; ];  
  rr:subjectMap [ rr:template "http://example.org/rsp/{person}"];
  rr:predicateObjectMap [ rr:predicate ex:isIn; 
    rr:objectMap [ rr:template "http://example.org/rsp/{room}"]].
        
