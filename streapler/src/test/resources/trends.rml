@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@prefix ex: <http://example.com/ns#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix ssn: <http://purl.oclc.org/NET/ssnx/ssn#>.
@prefix : <http://epfl.ch/mapping/>.

:TrendsMapping
  rml:logicalSource [
    rml:source "http://api.whatthetrend.com/api/v2/trends/active.json";
    rml:referenceFormulation ql:JSONPath;
    rml:iterator "$";    rml:root true;
    rml:dataPath "trends"; rml:timestamp "created_at"];

  rr:subjectMap [ 
    rr:template "http://trends.org/trend/{trend_id}"];
  rr:predicateObjectMap [ 
    rr:predicate ex:generatedAt; rr:objectMap [ rml:reference "created_at"]];
  rr:predicateObjectMap [ 
    rr:predicate ex:url; rr:objectMap [ rml:reference "url"]];
  rr:predicateObjectMap [ 
    rr:predicate ex:trendedAt; rr:objectMap [ rml:reference "last_trended_at"]];
  rr:predicateObjectMap [ 
    rr:predicate ex:category; rr:objectMap [rml:reference "category_name"]].
   
	